@page "{id}"
@model sadnerd.io.ATAS.OrderEventHub.Models.Pages.TopstepAccounts.DeleteModel
@{
    ViewData["Title"] = "Delete Topstep Account";
}

<h1>Delete Topstep Account</h1>

<p>Are you sure you want to delete this account?</p>

<form method="post">
    <input type="hidden" asp-for="TopstepAccount.TopstepAccountId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="/TopstepAccount/Index" class="btn btn-secondary">Cancel</a>
</form>
